{
  "name": "marked-annotated-hexdump",
  "version": "1.3.1",
  "description": "Create annotated hexdumps in markdown using markedjs or markdown-it",
  "type": "module",
  "keywords": [
    "marked",
    "markdown-it",
    "hexdump",
    "hex",
    "extension"
  ],
  "files": [
    "lib/",
    "src/"
  ],
  "exports": {
    "./marked": {
      "import": {
        "default": "./lib/marked.mjs",
        "types": "./lib/marked.d.ts"
      },
      "require": {
        "default": "./lib/marked.cjs",
        "types": "./lib/marked.d.ts"
      },
      "script": {
        "default": "./lib/marked.umd.js",
        "types": "./lib/marked.d.ts"
      }
    },
    "./markdown-it": {
      "import": {
        "default": "./lib/markdown-it.mjs",
        "types": "./lib/markdown-it.d.ts"
      },
      "require": {
        "default": "./lib/markdown-it.cjs",
        "types": "./lib/markdown-it.d.ts"
      },
      "script": {
        "default": "./lib/markdown-it.umd.js",
        "types": "./lib/markdown-it.d.ts"
      }
    }
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "format": "eslint --fix",
    "lint": "eslint",
    "test": "jest --verbose",
    "test:cover": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danishcake/marked-annotated-hexdump"
  },
  "author": "Edward Woolhouse <danishcake@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danishcake/marked-annotated-hexdump/issues"
  },
  "homepage": "https://github.com/danishcake/marked-annotated-hexdump#readme",
  "peerDependencies": {
    "markdown-it": ">=14 <15",
    "marked": ">=4 <15"
  },
  "peerDependenciesMeta": {
    "markdown-it": {
      "optional": true
    },
    "marked": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.25.7",
    "@markedjs/eslint-config": "^1.0.5",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/escape-html": "^1.0.4",
    "@types/markdown-it": "^14.1.2",
    "babel-jest": "^29.7.0",
    "codepage": "^1.15.0",
    "escape-html": "^1.0.3",
    "eslint": "^9.12.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "markdown-it": "^14.1.0",
    "marked": "^14.1.2",
    "rollup": "^4.24.0",
    "semantic-release": "^24.1.2",
    "tslib": "^2.8.0",
    "typescript": "^5.6.2"
  }
}
