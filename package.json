{
  "name": "marked-annotated-hexdump",
  "version": "1.0.0",
  "description": "Create annotated hexdumps in markdown using markedjs or markdown-it",
  "type": "module",
  "keywords": [
    "marked",
    "markdown-it",
    "hexdump",
    "hex",
    "extension"
  ],
  "files": [
    "lib/",
    "src/"
  ],
  "exports": {
    "./marked": {
      "import": "./lib/marked.mjs",
      "require": "./lib/marked.cjs",
      "script": "./lib/marked.umd.js"
    },
    "./markdown-it": {
      "import": "./lib/markdown-it.mjs",
      "require": "./lib/markdown-it.cjs",
      "script": "./lib/markdown-it.umd.js"
    }
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "format": "eslint --fix",
    "lint": "eslint",
    "test": "jest --verbose",
    "test:cover": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danishcake/marked-annotated-hexdump"
  },
  "author": "Edward Woolhouse <danishcake@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danishcake/marked-annotated-hexdump/issues"
  },
  "homepage": "https://github.com/danishcake/marked-annotated-hexdump#readme",
  "peerDependencies": {
    "markdown-it": ">=14 <15",
    "marked": ">=4 <15"
  },
  "peerDependenciesMeta": {
    "markdown-it": {
      "optional": true
    },
    "marked": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@markedjs/eslint-config": "^1.0.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.4",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/markdown-it": "^14.1.2",
    "babel-jest": "^29.7.0",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "markdown-it": "^14.1.0",
    "marked": "^14.0.0",
    "rollup": "^4.20.0",
    "semantic-release": "^24.0.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
